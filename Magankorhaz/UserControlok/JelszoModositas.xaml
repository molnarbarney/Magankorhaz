        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="JelszoModositas" Height="214" Width="300">
    <Grid>
        <Label Content="Régi jelszó:" HorizontalAlignment="Left" Margin="20,50,0,0" VerticalAlignment="Top"/>
        <Label Content="Új jelszó:" HorizontalAlignment="Left" Margin="20,80,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="oldPassword" HorizontalAlignment="Left" Height="23" Margin="102,54,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.5,-0.609" IsEnabled="False"/>
        <TextBox x:Name="newPassword" HorizontalAlignment="Left" Height="23" Margin="102,83,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="userLabel" Content="NÉV" HorizontalAlignment="Left" Margin="20,20,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.316,0.115"/>

    </Grid>
</Window>
